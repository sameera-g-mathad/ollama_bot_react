{
  "manifest_version": 3,
  "name": "ollama_bot",
  "version": "1",
  "description": "This is a Simple React app for connecting to a running Ollama to access LLM's",
  "action": {
    "default_popup": "index.html",
    "default_icons": {
      "16": "images/Logo_16.png",
      "32": "images/Logo_32.png",
      "48": "images/Logo_48.png",
      "128": "images/Logo_128.png"
    }
  }
}
